{
  "name": "starkbank/sdk",
  "version": "2.0.0",
  "type": "library",
  "description": "SDK to facilitate PHP integrations with Stark Bank",
  "homepage": "https://github.com/starkbank/sdk-php",
  "license": "MIT",
  "authors": [
    {
      "name": "StarkBank",
      "email": "developers@starkbank.com",
      "homepage": "https://starkbank.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "starkbank/ecdsa": "dev-master#0.0.2"
  },
  "keywords": [
    "starkbank",
    "bank",
    "open",
    "banking",
    "openbanking",
    "sdk"
  ],
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "StarkBank\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StarkBank\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "config": {
    "sort-packages": true
  }
}