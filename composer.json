{
  "name": "starkbank/sdk",
  "version": "2.12.0",
  "type": "library",
  "description": "SDK to facilitate PHP integrations with Stark Bank",
  "homepage": "https://github.com/starkbank/sdk-php",
  "license": "MIT",
  "authors": [
    {
      "name": "StarkBank",
      "email": "developers@starkbank.com",
      "homepage": "https://starkbank.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "starkinfra/core": "^0.1.0"
  },
  "keywords": [
    "starkbank",
    "bank",
    "open",
    "banking",
    "openbanking",
    "sdk"
  ],
  "autoload": {
    "files": ["src/init.php"],
    "psr-4": {
      "StarkBank\\": "src/"
    },
    "classmap": ["src/"]
  },
  "scripts": {
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "config": {
    "sort-packages": true
  }
}
