{
  "name": "starkbank/sdk",
  "version": "2.8.0",
  "type": "library",
  "description": "SDK to facilitate PHP integrations with Stark Bank",
  "homepage": "https://github.com/starkbank/sdk-php",
  "license": "MIT",
  "authors": [
    {
      "name": "StarkBank",
      "email": "developers@starkbank.com",
      "homepage": "https://starkbank.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "starkbank/ecdsa": "^0.0.5"
  },
  "keywords": [
    "starkbank",
    "bank",
    "open",
    "banking",
    "openbanking",
    "sdk"
  ],
  "autoload": {
    "files": ["src/init.php"]
  },
  "scripts": {
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "config": {
    "sort-packages": true
  }
